<style>
  .maternity-animated-header{
    background: #F5969E;
    position: relative;
    height: 75vh; 
    display: flex;
    overflow: hidden;
    box-sizing: border-box;
  }
  
  .maternity-animated-wrapper{
    width: auto;
    height: 100%;
    display: flex;
    justify-content: center;
    flex-direction: column;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
  }
  
  .image-left{
    height: 100%;
    width: 40%;
    float: left;
    text-align: center;
  }
  
  .image-left img{
    height: inherit;
  }
  
  .sub-title{
  	width: inherit;
    margin-top: 15px;
    text-align: end;
    color: white;
    white-space: nowrap;
    font-family: 'Montserrat', sans-serif;
  }
  
  #maternity-logo{
    animation: fill 1s ease forwards 4.5s;
  }

  #maternity-logo path:nth-child(1){
	stroke-dasharray: 754;
    stroke-dashoffset: 754;
  	animation: line-anim 1.5s ease forwards;
  }
  #maternity-logo path:nth-child(2){
	stroke-dasharray: 490;
    stroke-dashoffset: 490;
    animation: line-anim 1.5s ease forwards .4s;
  }
  #maternity-logo path:nth-child(3){
	stroke-dasharray: 336;
    stroke-dashoffset: 336;
    animation: line-anim 1.5s ease forwards .8s;
  }
  #maternity-logo path:nth-child(4){
	stroke-dasharray: 463;
    stroke-dashoffset: 463;
    animation: line-anim 1.5s ease forwards 1.2s;
  }
  #maternity-logo path:nth-child(5){
	stroke-dasharray: 549;
    stroke-dashoffset: 549;
    animation: line-anim 1.5s ease forwards 1.6s;
  }
  #maternity-logo path:nth-child(6){
	stroke-dasharray: 604;
    stroke-dashoffset: 604;
    animation: line-anim 1.5s ease forwards 2.0s;
  }
  #maternity-logo path:nth-child(7){
	stroke-dasharray: 226;
    stroke-dashoffset: 226;
    animation: line-anim 1.5s ease forwards 2.4s;
  }
  #maternity-logo path:nth-child(8){
	stroke-dasharray: 336;
    stroke-dashoffset: 336;
    animation: line-anim 1.5s ease forwards 2.8s;
  }
  #maternity-logo path:nth-child(9){
	stroke-dasharray: 372;
    stroke-dashoffset: 372;
    animation: line-anim 1.5s ease forwards 3.2s;
  }
  

  @keyframes line-anim {
    to {
      stroke-dashoffset: 0;
    }
  }
  @keyframes fill {
    from{
      fill: transparent;
    }to{
      fill: white;
    }
  }
  
  @media screen and (max-width: 1025px) {
    #maternity-logo{
      width: 85%;
      height: auto;
    }
    .maternity-animated-header{
      height: 25vh;
    }
  }
  
  @media screen and (min-width: 1700px){
    .maternity-animated-header{
        height: 50vh; 
    }
  }
</style>

