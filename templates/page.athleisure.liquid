<script>  
  $(document).ready(function(){
    $("#video-one").load();
    var video_button = $(".wrapper")[0];
    video_button.addEventListener("click", function() {
      var video = $("#video-one")[0];
      const booleanValue = video.muted.valueOf();
      if (booleanValue == true) {
        video.muted = false;
      } else {
        video.muted = true;
      }
    });
  });
</script>

<script>
  
var mybutton = document.getElementById("myBtn_onscroll");
window.onscroll = function() {scrollFunction()};

function scrollFunction() {
  if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
    mybutton.style.opacity = 1;
    mybutton.style.transform = "translateY(0px)";
  } else {
    mybutton.style.opacity = 0;
    mybutton.style.transform = "translateY(100px)";
  }
}

</script>


  




